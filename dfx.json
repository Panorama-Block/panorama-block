{
  "canisters": {
    "icp-bitcoin-ai-api": {
      "type": "azle",
      "main": "src/icp-bitcoin-ai-api/src/index.ts",
      "candid_gen": "automatic"
		},
    "hashblock": {
      "candid": "src/icp-bitcoin-ai-rust/hashblock/hashblock.did",
      "package": "hashblock",
      "type": "rust"
    },
    "addresses": {
      "candid": "src/icp-bitcoin-ai-rust/addresses/addresses.did",
      "package": "addresses",
      "type": "rust"
    },
    "core": {
      "candid": "src/icp-bitcoin-ai-rust/core/core.did",
      "package": "core",
      "type": "rust"
    },
    "db": {
      "candid": "src/icp-bitcoin-ai-rust/db/db.did",
      "package": "db",
      "type": "rust"
    },
    "icp-bitcoin-ai-frontend": {
      "dependencies": [
        "icp-bitcoin-ai-api",
        "internet_identity"
      ],
      "source": [
        "src/icp-bitcoin-ai-frontend/dist"
      ],
      "type": "assets",
      "workspace": "icp-bitcoin-ai-frontend"
    },
    "internet_identity": {
      "type": "custom",
      "candid": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity.did",
      "wasm": "https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_dev.wasm.gz",
      "remote": {
        "id": {
          "ic": "rdmx6-jaaaa-aaaaa-aaadq-cai"
        }
      },
      "frontend": {}
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
